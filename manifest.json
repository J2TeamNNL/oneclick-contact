{
	"manifest_version": 3,
	"name": "OneClick Contact",
	"version": "0.0.1",
	"description": "",
	"author": "taowork",
	"icons": {
		"16": "assets/favicon.png",
		"48": "assets/favicon.png",
		"128": "assets/favicon.png"
	},
	"background": {
		"service_worker": "sw.js"
	},
	"host_permissions": [
		"*://*/*"
	],
	"permissions": [
		"clipboardWrite",
		"clipboardRead",
		"contextMenus",
		"tabs",
		"notifications",
		"scripting",
		"storage",
		"unlimitedStorage"
	],
	"action": {
		"default_popup": "./popup/index.html"
	}
}
